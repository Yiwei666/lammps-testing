neighbor     0.8 bin

thermo_style custom step c_rot epair etotal press vol
thermo	     100

timestep     0.002

compute	     q all property/atom quatw quati quatj quatk

#dump	     1 all custom 100 dump.ellipse.resquared #	     id type x y z c_q[1] c_q[2] c_q[3] c_q[4]

